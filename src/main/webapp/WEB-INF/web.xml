<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.revature.servlet.LoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>NewUserServlet</servlet-name>
        <servlet-class>com.revature.servlet.NewUserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>EmpRbFormServlet</servlet-name>
        <servlet-class>com.revature.servlet.EmpRbFormServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>NewUserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>EmpRbFormServlet</servlet-name>
        <url-pattern>/EmpRbForm</url-pattern>
    </servlet-mapping>

</web-app>
